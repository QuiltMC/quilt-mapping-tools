QUILT_MAPPING	<from_namespace>	[to_namespace]
	EXTENSIONS	unpick	comment	transitive
CLASS	<from_name>	[to_name]
	COMMENT	This is a test comment!
	FIELD	<from_name>	[to_name]	<from_descriptor>
		UNPICK	<test_group>	constant
	CLASS	<nested_from_name>	[nested_to_name]
	METHOD	<from_name>	[to_name]	<from_descriptor>
		ARG	0	[name]
			UNPICK	<test_group>
		ARG	1	[name]
			UNPICK	<test_group>
		RETURN
			UNPICK	<test_group>
	METHOD	<from_name_transitive>	<from_descriptor_transitive>
		TRANSITIVE	<from_name>
